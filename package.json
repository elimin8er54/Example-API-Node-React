{
  "name": "exampleapi",
  "version": "1.0.0",
  "description": "A full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/dist/index.js",
  "scripts": {
    "build": "tsc -p src/client && webpack --mode production",
    "start": "npm run build && tsc -p src/server && node src/server/dist/index.js",
    "client": "tsc -p src/client && webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "tsc -p src/server && nodemon src/server/dist/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest && node src/server/controllers/property.controller.js"
  },
  "author": "Shaunt Keshishian",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
